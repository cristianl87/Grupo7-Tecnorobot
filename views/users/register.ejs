    <link rel="stylesheet" href="/css/styles.css">
    <link rel="stylesheet" href="/css/normalize.css">
</head>
<body>
    <header class="header-home">
        <div class="contenedor-header">
            <section class="logo">
                <img src="/images/logo-mobile.jpg" alt="Logo TecnoRobot" class="logo-mobile">
                <img src="/images/TecnoRobot.jpg" alt="Logo TecnoRobot" class="tablet">
            </section>
            <section class="barra-busqueda">
                <i class="fas fa-search"></i>
                <input type="text" placeholder="Buscar...">
            </section>
            <section class="iconos-header">
                <i class="fas fa-shopping-cart"></i>
                <nav class="mobile">
                    <i class="fas fa-ellipsis-v"></i>
                </nav>
                <i class="fas fa-user user-account-icon"></i>
            </section>
        </div>
    </header>
    <nav class="desktop">
        <ul>
            <li> <a href="#"> Inicio </a> </li>
            <li> <a href="#"> Categorías </a> </li>
            <li> <a href="#"> Marcas </a> </li>
            <li> <a href="#"> Cursos </a> </li>
            <li> <a href="#"> Nosotros </a> </li>
            <li> <a href="#"> Tienda </a> </li>
        </ul>
    </nav> 
    <form class="form-register" action="/login" method="POST">
        <span class="iniciar-sesion" ><strong>CREAR CUENTA</strong> </span>
        <div class="formulario">
        <input class="nombre" type="text" name="user" placeholder="Nombre de usuario">
        </div>
        <div class="">
            <input class="email" type="email" name="user" placeholder="Correo electrónico">
            </div>
        <div class="formulario">
            <input class="contraseña" type="password" name="password" placeholder="Ingresá contraseña">
            <label class="info"><i class="fas fa-info-circle"></i> La contraseña debe contener al menos seis caracteres.</label>
        </div> 
        <div class="formulario">
            <input class="contraseña" type="password" name="password" placeholder="Repita su contraseña">
            <label class="info"></label>
        </div> 
        <div class="formulario"> 
            <button class="boton-register" type="submit" value="">Crea tu cuenta de Tecnorobot </button>
        </div>
        <span class="text-register">Al crear una cuenta, aceptas las Condiciones de Uso y el Aviso de Privacidad de Tecnorobot.</span>
        
    </form>
   
    </main>
    <footer class="footer">
        <span class="wave1"></span>
        <section class="social-icons">
            <a href="#">
                <i class="far fa-envelope"></i>
            </a>
            <a href="#">
                <i class="fab fa-facebook"></i>
            </a>
            <a href="#">
                <i class="fab fa-instagram"></i>
            </a>
        </section>
        <nav class="nav-footer">
            <ul>
                <li> <a href="#"> Inicio </a> </li>
                <li> <a href="#"> Categorías </a> </li>
                <li> <a href="#"> Marcas </a> </li>
                <li> <a href="#"> Cursos </a> </li>
                <li> <a href="#"> Nosotros </a> </li>
                <li> <a href="#"> Tienda </a> </li>
            </ul>
        </nav>
        <p class="copy">&copy; 2021 Tecnorobot USA, Inc. All rights reserved.</p>
    </footer>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
